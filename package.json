{
  "name": "movie-manager",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .dev.env vite --host",
    "generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js",
    "prod": "env-cmd -f .prod.env vite --host",
    "copy-sw": "cp service-worker.js dist/service-worker.js",
    "build:dev": "env-cmd -f .dev.env vite build && workbox injectManifest workbox-config.cjs",
    "build:prod": "env-cmd -f .prod.env vite build && workbox injectManifest workbox-config.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve": "vite serve"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/styles": "^5.14.20",
    "axios": "^1.6.2",
    "env-cmd": "^10.1.0",
    "formik": "^2.4.5",
    "localstorage-slim": "^2.7.0",
    "react": "^18.2.0",
    "react-cache-buster": "^0.1.8",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-toastify": "^10.0.5",
    "use-debounce": "^10.0.0",
    "vite-plugin-pwa": "^0.20.0",
    "workbox-core": "^7.1.0",
    "workbox-window": "^7.1.0",
    "yup": "^1.3.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "vite": "^5.0.0"
  }
}
